<article class="article article-links article-type-list" itemscope="">
    <header class="article-header">
      <h1 itemprop="title"><%= page.title %></h1>
      <p class="text-muted">
        <%= __('page.navs-desc') %>
      </p>
    </header>
    <div class="article-body">
      <div class="row">
        <% for (var nav in site.data['site-navs']) { %>
          <% var item = site.data['site-navs'][nav]; %>
            <div class="col-sm-6 col-md-4">
                  <div class="media-body p-0x">
                    <h4 class="media-heading"><a href="<%= item.link %>" target="_blank"><%= item.desc %></a></h4>
                  </div>
              </div>
            <% } %>
      </div>
    </div>
  </article>
  <% if (theme.comment.type && !is_home()) { %>
    <%- partial('_partial/post/comment', {post: page}) %>
  <% } %>
  